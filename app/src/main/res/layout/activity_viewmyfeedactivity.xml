<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".viewmyfeedactivity">

    <TextView
        android:id="@+id/allmyfeedsfield"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/fira_mono_bold"
        android:text="All My Feeds"
        android:textAlignment="center"
        android:textColor="@android:color/holo_blue_dark"
        android:textFontWeight="800"
        android:textSize="25dp"
        android:textStyle="bold"></TextView>

    <ListView
        android:id="@+id/listviewoffeed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/allmyfeedsfield"></ListView>


</RelativeLayout>